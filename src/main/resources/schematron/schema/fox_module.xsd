                            <xs:annotation>
                              <xs:documentation>hh:mm:ss</xs:documentation>
                            </xs:annotation>
                          </xs:enumeration>
                        </xs:restriction>
                      </xs:simpleType>
                    </xs:attribute>
                    <xs:attribute name="sql-type">
                      <xs:annotation>
                        <xs:documentation>The sql datatype in the database. If not entered then FOX
